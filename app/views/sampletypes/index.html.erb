<% provide(:title, 'All Sample Types ') %>
<h1>All Common Names</h1>

<%= will_paginate %>

<ul class="Sample Types">
  <% @sampletypes.each do |sampletype| %>
    <li>
      <%= link_to sampletype.name, sampletype %>
      | <%= link_to "edit", edit_sampletype_path(sampletype) %>
      <% if current_user.admin? %>
    	| <%= link_to "delete", sampletype, method: :delete,
                                  data: { confirm: "You sure you wish to delete this Sample Type?" } %>
  	   <% end %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>