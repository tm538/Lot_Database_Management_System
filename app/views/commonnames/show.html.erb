<% provide(:title, @commonname.name) %>
<h1>
	<%= @commonname.name %>
</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@commonname) do |f| %>
	  <%= render 'shared/error_messages', object: f.object %>
	
	  <%= f.label :name %>
	  <%= f.text_field :name, :readonly => true %>
	  
    <% end %>
  </div>
</div>