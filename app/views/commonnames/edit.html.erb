<% provide(:title, 'Update Common Name') %>
<h1>Update Common Name</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@commonname, :html => { :class => "form-horizontal"}) do |f| %>
	  <%= render 'shared/error_messages', object: f.object %>

	  <%= render :partial => 'commonnames/form', :locals => { :f => f } %>
	  
      <div class="control-group">
		<div class="controls">	
      		<%= f.submit "Update Common Name", class: "btn btn-warning" %>
      	</div>
      </div>
      
    <% end %>
  </div>
</div>