<% provide(:title, 'Update Common Name') %>
<h1>Update Common Name</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@commonname) do |f| %>
	  <%= render 'shared/error_messages', object: f.object %>

	  <%= f.label :name %>
	  <%= f.text_field :name %>

      <%= f.submit "Update Common Name", class: "btn btn-large btn-warning" %>
      
    <% end %>
  </div>
</div>